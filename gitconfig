[alias]
    d = diff
    dt = difftool
    b = branch
    bd = !git branch -D
    rbd = !git push origin --delete
    r = remote
    ra = remote add
    rr = remote rm
    rv = remote -v
    co = checkout
    cot = !git checkout --track
    cob = !git checkout -b
    pu = !git push -u
    cl = clone
    sa = submodule add
    c = !git commit -a
    cm = !git commit -am
    ca = !git commit --amend
    s = status
    m = merge
    mt = mergetool
    l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    lh = !git log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit | head
    a = add
    p = push
    u = pull
    f = fetch
    st = stash
    sts = stash save
    sta = stash apply
    std = stash drop
    stl = stash list
    stc = stash clear
    stp = stash pop
[push]
	default = tracking
